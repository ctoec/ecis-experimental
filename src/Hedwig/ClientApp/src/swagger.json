          }
        },
        "additionalProperties": false
      },
      "User": {
        "required": [
          "firstName",
          "lastName"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "wingedKeysId": {
            "type": "string",
            "format": "uuid"
          },
          "firstName": {
            "maxLength": 35,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "middleName": {
            "maxLength": 35,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "lastName": {
            "maxLength": 35,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "suffix": {
            "maxLength": 10,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "orgPermissions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/OrganizationPermission"
            },
            "nullable": true
          },
          "sitePermissions": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/SitePermission"
            },
            "nullable": true
          },
          "sites": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Site"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Funding": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "enrollmentId": {
            "type": "integer",
            "format": "int32"
          },
          "enrollment": {
            "$ref": "#/components/schemas/Enrollment"
          },
          "source": {
            "$ref": "#/components/schemas/FundingSource"
          },
          "time": {
            "$ref": "#/components/schemas/FundingTime"
          },
          "authorId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "author": {
            "$ref": "#/components/schemas/User"
          }
        },
        "additionalProperties": false
      },
      "Enrollment": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "childId": {
            "type": "string",
            "format": "uuid"
          },
          "child": {
            "$ref": "#/components/schemas/Child"
          },
          "siteId": {
            "type": "integer",
            "format": "int32"
          },
