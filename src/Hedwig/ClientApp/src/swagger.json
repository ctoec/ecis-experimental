        "type": "string"
      },
      "FundingTime": {
        "enum": [
          "Full",
          "Part"
        ],
        "type": "string"
      },
      "OrganizationPermission": {
        "type": "object",
        "properties": {
          "organizationId": {
            "type": "integer",
            "format": "int32"
          },
          "organization": {
            "$ref": "#/components/schemas/Organization"
          },
          "id": {
